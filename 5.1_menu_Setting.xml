				<CONTROL name = "Set_restart_mode_selection_Checkbox1" xpos = "150" ypos = "10" width = "80" fieldtype = "checkbox"  fontpixelsize = "10" refvar = "nck/Channel/GeometricAxis/actProgPos[1]">
