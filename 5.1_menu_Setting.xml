				<control name="Force_Value1" xpos="410" ypos="10" width="35" height="20" refvar="nck/Channel/GeometricAxis/progDistToGo[2]" fieldtype="readonly"  format="%9.2f" fontpixelsize = "12" color_bk = "#FFFFFF"/>
